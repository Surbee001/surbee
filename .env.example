# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# xAI Grok API Configuration
# Get your API key from: https://console.x.ai/
XAI_API_KEY=your_xai_api_key_here
XAI_API_BASE_URL=https://api.x.ai/v1

# AI Model Provider API Keys
# Anthropic Claude API - Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Mistral AI API - Get your API key from: https://console.mistral.ai/
MISTRAL_API_KEY=your_mistral_api_key_here

# E2B Code Interpreter - Get your API key from: https://e2b.dev/
E2B_API_KEY=your_e2b_api_key_here

# Optional: Mock mode for development without backend
NEXT_PUBLIC_MOCK_PROJECT=false

# Analytics Admin Key - Required to view the admin analytics dashboard
# Generate a secure random key for production
ANALYTICS_ADMIN_KEY=your_secure_admin_key_here

